    <level4 id="wsb_13302006">
      <front>
        <biblioData type="L">
          <title>
            <mainTitle>肅宗補闕正誤 33年 2月 6日</mainTitle>
          </title>
          <date>
            <dateOccured date="1707-02-06L0" type="서기"/>
            <dateOccured type="간지">己丑</dateOccured>
            <dateOccured type="재위연도">숙종 [보궐정오실록] 33년 2월 6일</dateOccured>
            <dateOccured type="개국연호">조선 개국(開國) 316년 2월 6일</dateOccured>
            <dateOccured type="중국연호">청 강희(康熙) 46년 2월 6일</dateOccured>
            <dateOccured type="단기">4040년 2월 6일</dateOccured>
            <dateOccured type="일본연호">년 2월 6일</dateOccured>
