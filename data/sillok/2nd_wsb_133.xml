    <level4 id="wsb_13312003">
      <front>
        <biblioData type="L">
          <title>
            <mainTitle>肅宗補闕正誤 33年 12月 3日</mainTitle>
          </title>
          <date>
            <dateOccured date="1707-12-03L0" type="서기"/>
            <dateOccured type="간지">辛巳</dateOccured>
            <dateOccured type="재위연도">숙종 [보궐정오실록] 33년 12월 3일</dateOccured>
            <dateOccured type="개국연호">조선 개국(開國) 316년 12월 3일</dateOccured>
            <dateOccured type="중국연호">청 강희(康熙) 46년 12월 3일</dateOccured>
            <dateOccured type="단기">4040년 12월 3일</dateOccured>
            <dateOccured type="일본연호">년 12월 3일</dateOccured>
