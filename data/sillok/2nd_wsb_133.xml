      <level5 id="wsb_13311005_001">
        <front>
          <biblioData type="T">
            <title>
              <mainTitle>병조 판서 이인엽이 이교악 등의 말로 인해 사직하는 상소하다</mainTitle>
            </title>
            <docNo level="n" name="titleno">001</docNo>
            <date>
              <dateOccured date="1707-11-05L0" type="서기"/>
            </date>
            <source>
              <mainTitle type="태백산사고본">肅宗實錄補闕正誤 52책 45권</mainTitle>
              <page begin="5장 B면"/>
            </source>
            <source>
              <mainTitle type="국편영인본">肅宗實錄補闕正誤 40책</mainTitle>
              <page begin="285면"/>
