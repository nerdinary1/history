      <level5 id="wsb_12902014_001">
        <front>
          <biblioData type="T">
            <title>
              <mainTitle>전 판서 오도일의 졸기</mainTitle>
            </title>
            <docNo level="n" name="titleno">001</docNo>
            <date>
              <dateOccured date="1703-02-14L0" type="서기"/>
            </date>
            <source>
              <mainTitle type="태백산사고본">肅宗實錄補闕正誤 44책 38권</mainTitle>
              <page begin="1장 B면"/>
