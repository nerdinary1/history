      <level5 id="wsb_12902015_001">
        <front>
          <biblioData type="T">
            <title>
              <mainTitle>이익수를 공조 참판으로 삼다</mainTitle>
            </title>
            <docNo level="n" name="titleno">001</docNo>
            <date>
              <dateOccured date="1703-02-15L0" type="서기"/>
            </date>
            <source>
              <mainTitle type="태백산사고본">肅宗實錄補闕正誤 44책 38권</mainTitle>
              <page begin="1장 B면"/>
            </source>
            <source>
              <mainTitle type="국편영인본">肅宗實錄補闕正誤 40책</mainTitle>
