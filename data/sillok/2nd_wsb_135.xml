      <level5 id="wsb_13501016_001">
        <front>
          <biblioData type="T">
            <title>
              <mainTitle>윤증을 우의정으로 삼다</mainTitle>
            </title>
            <docNo level="n" name="titleno">001</docNo>
            <date>
              <dateOccured date="1709-01-16L0" type="서기"/>
            </date>
            <source>
              <mainTitle type="태백산사고본">肅宗實錄補闕正誤 54책 47권</mainTitle>
              <page begin="1장 A면"/>
            </source>
