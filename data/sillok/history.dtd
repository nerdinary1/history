<!-- *****************************************************  -->
<!-- ====== Start Entity Declaration ================= -->
<!-- 2015.07 작업자: coreawin@diquest.com 추가 : 태백산사고본 | 정족산사고본 | 오대산사고본 | 국편영인본 | 원본 | 원전-->
<!ENTITY % title "기사명 |문서명 |문서철명 | 하위문서철명 | 참조문서명 | 주제목 | 소제목 | 태백산사고본 | 정족산사고본 | 오대산사고본 | 국편영인본 | 원본 | 원전| 탈초본 | 개수전거">
<!-- 2015.08 @junebm 추가 : : 개수전거 (승정원일기)
-->
<!ENTITY % date "dateSend | dateReceived | dateOccured | dateCreated | dateContract">
<!ENTITY % pDate "발송일 | 접수일 | 발생일 | 작성일 | 계약일 | 처리일 | 수신일 | 게재일">
<!ENTITY % creator "author |editor | sender | receiver | reporter | binder | translator | contributor">
<!ENTITY % role "저자 | 필자 | 생산기관 | 편집자 | 편찬자 | 발송자 | 발수신 |  발신자 | 수신자 | 작성자 | 결재자 | 계약자 | 신문자 | 등본인 | 입회인 | 통역 | 서기 | 피의자 | 피고인 | 증인 | 참고인 | 발언자 | 공술자 | 진술인 | 번역자 | 광고주 |촬영자 | 화가 | 개수자 | 찬자">
<!--@2015.7. @junebm; 한국사콘텐츠팀 요청으로 촬영자, 화가 추가-->
<!--@2015.8. @junebm; 승정원일기 변환을 위해 개수자, 찬자 추가-->
<!ENTITY % p.index "이름 | 지명 | 관직 | 서명 | 관서 | 단체 | 사건 | 국명 | 연호 | 학교 | 회사조합 | 기타">
<!-- 2015.07 작업자: coreawin@diquest.com 추가 : proofreading -->
<!ENTITY % p.note "annotation | qna| point | missing | seal | quotation | proofreading">
<!-- 2015.07 작업자: coreawin@diquest.com 추가 : 악보 
 2015.11 작업자: @junebm 추가 : 탈자 
	-->
<!ENTITY % note "교감주 |편자주 | 두주 | 원주 | 탈초자주 | 난외주 | 간주 |  각주 | 소자쌍행 | 번역주 | 원저자주 | 도말 | 낙점 | 연문 | DB주석 | 악보 | 개수전거|탈자">
<!ENTITY % list " ul | ol">
<!ENTITY % p.zz.ph " index |newChar | number | emph | sup | sub | br | %p.note;">
<!ENTITY % p.zz.cont "pTitle | name | pDate | index | newChar | reference | tableGroup | table | illustration | image | number | place | attach | emph | sup | sub | br |%list; | %p.note; | explanation | link">
<!--2015.08 @junebm; 편의상 승정원일기 reamrk의 type 속성 "근무현황"은 pace 태그를 사용하기로 함.
	  승정원일기 remark의  type 속성 "부연설명"은 explanation 태그를 사용하기로 하고 explanation이 paragraph 하위에 사용될 수 있게 함
-->
<!ENTITY % h.align "(left|center|right) 'center' ">
<!ENTITY % v.align "(top|middle|bottom) 'middle' ">
<!ENTITY % li.type "(solid | dot | dash | omit)">
<!-- ====== End Entity Declaration =================== -->
<!-- ====== Start Element Declaration ================ -->
<!-- ***********************************************  -->
<!--			 item : 자료                                               	  -->
<!-- ***********************************************  -->
<!ELEMENT item (front?, level1+)>
<!-- front: 개요 등, body: 본문 -->
<!ATTLIST item
	id CDATA #REQUIRED
	name CDATA #IMPLIED
	org CDATA #FIXED "국사편찬위원회"
>
<!--
	// itemID삭제 
	// itemName삭제, name으로 대용
 -->
<!-- ============================================   -->
<!--			front : 서지, 기술사항, 원문이미지  		-->
<!-- ============================================   -->
<!ELEMENT front (biblioData?, description?, illustGroup?, service?)>
<!-- ***************************************************************************** -->
<!--  		biblioData : 서명, 작성자, 날짜, 발행, 소장정보, 출전, 주제명, 물리적기술사항  -->
<!--  		description : 서문, 범례, 해설, 초록, 해제, 간행후기, 대상시기                               -->
<!--  		illustGroup: 원문이미지                                                                              -->
<!-- ***************************************************************************** -->
<!ELEMENT biblioData (title, docNo*, creator?, date?, publication?, binderPeriod?, holdings?, source*, subjectClass*, physicalDescription?, fileDesc?)>
<!ATTLIST biblioData
	type CDATA #IMPLIED
>
<!ELEMENT fileDesc (#PCDATA)>
<!--
	// id : 삭제
	// type : 탈초, 원문, 번역문, ...
-->
<!-- 1. type : 원문, 번역문, 주석문. -->
<!-- 2. type : 잡지의 목차제공잡지, 원문제공잡지. -->
<!--
@add 2015-08
@modifier coreawin@diquest.com
	for 승정원 일기
	element add : weather
-->
<!ELEMENT description (introduction*, remarks*, commentary*, abstract?, biblioExplanation?, afterNote?, tableOfContent?, coveragePeriod?, newsInfo?, weather?)>
<!ELEMENT illustGroup (illustration)*>
<!ELEMENT service (accessCondition, metaOperator*, dateInsert?, dateModified?)>
<!-- 서비스: 웹서비스 여부, 메타데이터 작성자, 데이터입력일, 데이터수정일 -->
<!ELEMENT level1 (front?, text?, level2*)>
<!ATTLIST level1
	id CDATA #REQUIRED
	type CDATA #IMPLIED
	value CDATA #IMPLIED
	왕명 CDATA #IMPLIED
>
<!-- 
	level1 : 물리적인 권을 기본으로 함
	@type = 문서류 : 문서철
			연표, 신문 : 년
			목록 : 소장처(om)
	tcko : 왕명 
-->
<!ELEMENT level2 (front?, text?, level3*)>
<!ATTLIST level2
	id CDATA #REQUIRED
	type CDATA #IMPLIED
	value CDATA #IMPLIED
	간지 CDATA #IMPLIED
	왕명 CDATA #IMPLIED
	재위년도 CDATA #IMPLIED
	중국년도 CDATA #IMPLIED
	일본년도 CDATA #IMPLIED
>
<!-- type (문서철 | 하위문서철 | 문서 | 년 | 월 | 대표지명 | 지도명 ) #IMPLIED -->
<!-- 
	level2
	@type = 문서류 : 문서철, 하위문서철, 문서 
			편년류 : 년 , 월
			연표,신문 : 월
			지도 : 대표지명, 지도명
	bb : 간지, 왕명, 재위년도
	gj : 간지, 왕명, 일본년도, 재위년도, 중국년도
-->
<!ELEMENT level3 (front?, text?, level4*)>
<!ATTLIST level3
	id CDATA #REQUIRED
	value CDATA #IMPLIED
	type CDATA #IMPLIED
	간지 CDATA #IMPLIED
	왕명 CDATA #IMPLIED
	재위년도 CDATA #IMPLIED
>
<!--type ( 하위문서철 | 문서 | 년 | 월 | 일 | 지도명 ) #IMPLIED-->
<!-- 
	level3
	@type = 문서류 : 하위문서철, 문서 
			편년류 : 년 , 월
			연표,신문 : 일 
			지도 : 지도명
	ks : 간지, 왕명, 재위년도
-->
<!ELEMENT level4 (front?, text?, level5*)>
<!ATTLIST level4
	id CDATA #REQUIRED
	value CDATA #IMPLIED
	type CDATA #IMPLIED
>
<!--type ( 문서 | 월 | 일 | 좌목  ) #IMPLIED-->
<!-- 
	level4
	@type = 문서류 : 문서 
			편년류 : 월(각사등록), 일, 좌목
-->
<!ELEMENT level5 (front?, text?, level6*)>
<!ATTLIST level5
	id CDATA #REQUIRED
	type CDATA #IMPLIED
	value CDATA #IMPLIED
>
<!--type ( 일 ) #IMPLIED-->
<!-- 
	level5
	@type = 편년류 : 일(각사등록)
-->
<!ELEMENT level6 (front?, text)>
<!ATTLIST level6
	id CDATA #REQUIRED
>
<!-- *********************************************** -->
<!-- 			text : 원본내용	                                   -->
<!-- *********************************************** -->
<!ELEMENT text (link*, content?, attach*, annex*)>
<!ELEMENT content (postScript | paragraph | quotation | annotation | manuscript)*>
<!ATTLIST content
	type CDATA #IMPLIED
>
<!--
	// id 삭제
	// type 없는지 테스트
-->
<!-- content 는 paragraph으로 구성되어야 함 -->
<!-- 변환이 어렵다면 기존처럼  <!ELEMENT content (#PCDATA | %p.zz.ph; | paragraph+)> 방식으로 가야하는데 지저분함 -->
<!ELEMENT paragraph (#PCDATA | %p.zz.cont;)*>
<!ATTLIST paragraph
	indent CDATA #IMPLIED
	align %h.align; 
>
<!ELEMENT manuscript (#PCDATA | paragraph)*>
<!ATTLIST manuscript
	id CDATA #IMPLIED
	name CDATA #IMPLIED
>
<!--@2015.7. @junebm; name 속성 #fixed "탈초"를  #IMPLIED로 변경. 본문 내 별도 문서가 삽입되는 경우, 즉 content 내 별도 문서 삽입의 경우    manuscript 요소를 사용하기로 함. 한국사콘텐츠 팀의 문서 내 관련 사료 삽입 처리 요청에 따름. -->
<!-- ************************************************** -->
<!-- 			postScript :  추신    	                    -->
<!-- ************************************************** -->
<!ELEMENT postScript (caption?, paragraph*)>
<!ATTLIST postScript
	type CDATA #IMPLIED
>
<!-- *********************************************** -->
<!-- 			link : 연결 정보                                              -->
<!-- *********************************************** -->
<!ELEMENT link (#PCDATA)>
<!ATTLIST link
	type (original | copy | korean) #IMPLIED
	uri (url | hoi | doi | uci | gps | coi |sillok |sjw |akh) #IMPLIED
	value CDATA #IMPLIED
	src CDATA #IMPLIED
	org CDATA #IMPLIED
>
<!--@2015.11.06 uri 값에 아예 실록, 승정원일기, 전자사료관 추가. sillok, sjw,akh 
@2015.7. @junebm; 한국사콘텐츠팀 요청에 따라 uri 속성에 coi 추가. 참고 hoi는 한국사데이터베이스 id 체계. coi는 우리역사넷 id 체계 -->
<!-- 			
	type : original(원문), copy(영인본/사본), korean(국역본)
	uri : 
	value : uri 타입에 따른 값
			url = url주소
			hoi = 한국사데이터베이스 ID, 단, gskh, gskh, jm은 별도 ID체계. sillok, sjw로 시작하는 ID는 실록, 승정원일기. 별도 처리.
			doi = doi ID
			uci = uci ID
			gps = 위도,경도 
			coi = 우리역사넷 ID
	src : 출처정보
	org : 기관명
-->
<!-- *********************************************** -->
<!-- ************************************************** -->
<!-- 			attach :  첨부    	                                       -->
<!-- ************************************************** -->
<!ELEMENT attach (caption?, file?, explanation?, reference?)>
<!ATTLIST attach
	docNo CDATA #IMPLIED
	begin CDATA #IMPLIED
	end CDATA #IMPLIED
>
<!ELEMENT file EMPTY>
<!ATTLIST file
	src CDATA #REQUIRED
	type CDATA #IMPLIED
>
<!-- ************************************************** -->
<!-- 			annex :  별지    	                                           -->
<!-- ************************************************** -->
<!ELEMENT annex (front?, text?)>
<!ATTLIST annex
	id CDATA #IMPLIED
	type CDATA #IMPLIED
	level NMTOKEN "n"
	name CDATA #IMPLIED
>
<!-- level은  1: 별지, 2: 별지의 부속서 -->
<!-- ====================================================   -->
<!-- 			front를 위한 엘리먼트 	          	                            -->
<!-- ====================================================   -->
<!--  		biblioData : 서명, 작성자, 날짜, 발행, 소장정보, 출전, 주제명, 물리적기술사항          -->
<!-- ************************************** -->
<!-- 			title 											 -->
<!-- ************************************** -->
<!ELEMENT title (mainTitle, seriesTitle?, alternative*)>
<!ELEMENT mainTitle (#PCDATA | newChar)*>
<!ATTLIST mainTitle
	type (%title;) #IMPLIED
	volume CDATA #IMPLIED
	lang CDATA #IMPLIED
>
<!ELEMENT seriesTitle (#PCDATA | newChar)*>
<!ATTLIST seriesTitle
	type (총서명 | 자료집명) #IMPLIED
	volume CDATA #IMPLIED
	lang CDATA #IMPLIED
>
<!ELEMENT alternative (#PCDATA | newChar)*>
<!ATTLIST alternative
	type CDATA #IMPLIED
	lang CDATA #IMPLIED
>
<!-- @2015.7 @junbem; 한국사콘텐츠팀의 요청으로 에서 type 속성 CDATA로 변경 -->
<!-- ************************************** -->
<!-- 			creator 					                     -->
<!-- ************************************** -->
<!ELEMENT creator (%creator;)*>
<!--  저필자, 편자, 발송자, 수신자, 번역자, 작성자(그림, 사진 등 포함), 편철기관  -->
<!ELEMENT author (name)*>
<!ATTLIST author
	position CDATA #IMPLIED
>
<!ELEMENT editor (name)*>
<!ATTLIST editor
	position CDATA #IMPLIED
>
<!ELEMENT sender (name)*>
<!ATTLIST sender
	position CDATA #IMPLIED
>
<!ELEMENT receiver (name)*>
<!ATTLIST receiver
	position CDATA #IMPLIED
>
<!ELEMENT translator (name)*>
<!ATTLIST translator
	position CDATA #IMPLIED
>
<!ELEMENT reporter (name)*>
<!ATTLIST reporter
	position CDATA #IMPLIED
>
<!ELEMENT binder (name)*>
<!ELEMENT contributor (name)*>
<!ATTLIST contributor
	position CDATA #IMPLIED
	role CDATA #IMPLIED
>
<!-- contributor에는 작성자 중 결재자 | 계약자 | 신문자 | 등본인 | 입회인 | 통역 등을 기술  -->
<!-- contributor의 role 속성 값에 기술  -->
<!ELEMENT name (#PCDATA | %p.zz.ph;)*>
<!ATTLIST name
	lang CDATA #IMPLIED
	align %h.align;
	role (%role;) #IMPLIED 
>
<!-- ************************************** -->
<!-- 			date  						              	 -->
<!-- ************************************** -->
<!ELEMENT date (%date;)*>
<!ELEMENT dateCreated (#PCDATA)>
<!ATTLIST dateCreated
	date CDATA #IMPLIED
	type CDATA #IMPLIED
>
<!ELEMENT dateSend (#PCDATA)>
<!ATTLIST dateSend
	date CDATA #IMPLIED
	type CDATA #IMPLIED
>
<!ELEMENT dateReceived (#PCDATA)>
<!ATTLIST dateReceived
	date CDATA #IMPLIED
	type CDATA #IMPLIED
>
<!ELEMENT dateOccured (#PCDATA | annotation)*>
<!ATTLIST dateOccured
	date CDATA #IMPLIED
	type CDATA #IMPLIED
>
<!ELEMENT dateContract (#PCDATA)>
<!ATTLIST dateContract
	date CDATA #IMPLIED
	type CDATA #IMPLIED
>
<!-- ************************************************** -->
<!--   발행, 소장정보, 출전, 주제명, 물리적기술사항 			 -->
<!-- ************************************************** -->
<!ELEMENT publication (publisher?, place?, dateIssued?)>
<!ELEMENT publisher (#PCDATA | %p.zz.ph;)*>
<!ELEMENT place (#PCDATA | %p.zz.cont;)*>
<!ATTLIST place
	type CDATA #IMPLIED
>
<!-- place의 type에는 신문장소 등-->
<!ELEMENT dateIssued (#PCDATA)>
<!ATTLIST dateIssued
	date CDATA #IMPLIED
	begin CDATA #IMPLIED
	end CDATA #IMPLIED
	class (조간 | 석간 | 무 | 부록 | 호외) #IMPLIED
>
<!-- 잡지 및 신문의 창간일 begin, 폐간일 end -->
<!-- 신문의 발행구분(조간, 석간 등) 은 class로 -->
<!ELEMENT docNo (#PCDATA | newChar)*>
<!ATTLIST docNo
	level NMTOKEN "n"
	name CDATA #IMPLIED
>
<!-- 문서번호, 발송번호, 각사등록근대편 문건형태 -->
<!ELEMENT binderPeriod EMPTY>
<!ATTLIST binderPeriod
	begin CDATA #REQUIRED
	end CDATA #REQUIRED
>
<!ELEMENT holdings (originOwner?, owner?, history?, serialNumber?)>
<!-- 원소장처, 현소장처, 소장이력, 등록정보-->
<!ELEMENT originOwner (#PCDATA)>
<!ELEMENT owner (#PCDATA)>
<!ATTLIST owner
	registrationNo CDATA #IMPLIED
	callNo CDATA #IMPLIED
>
<!-- 소장처의 도서 등록번호 및 청구번호 예) 규장각청구번호 -->
<!ELEMENT history (#PCDATA)>
<!ELEMENT serialNumber EMPTY>
<!ATTLIST serialNumber
	registrationNo CDATA #IMPLIED
	MFNo CDATA #IMPLIED
	callNo CDATA #IMPLIED
>
<!ELEMENT source (mainTitle?, dateIssued?, page?)>
<!ELEMENT page EMPTY>
<!ATTLIST page
	begin CDATA #IMPLIED
	end CDATA #IMPLIED
>
<!ELEMENT subjectClass (#PCDATA)>
<!ATTLIST subjectClass
	scheme CDATA #IMPLIED
	code CDATA #IMPLIED
	level CDATA #IMPLIED
>
<!-- level : 주제의 depth. 예) 중추원조사자료의 주제명과 그룹명, 중국정사조선전의 대분류, 중분류 -->
<!ELEMENT physicalDescription (originSize?, originQantity?, originForm?, originLang?, originType?)>
<!--  원본의 크기, 분량, 형태, 언어, 판본사항 -->
<!ELEMENT originSize (#PCDATA)>
<!ELEMENT originQantity (#PCDATA)>
<!ELEMENT originForm (#PCDATA)>
<!ELEMENT originLang (#PCDATA)>
<!ATTLIST originLang
	lang CDATA #IMPLIED
>
<!ELEMENT originType (#PCDATA)>
<!-- ********************************************************* -->
<!-- 	description : 서문, 범례, 해설, 해제, 대상시기  -->
<!-- ********************************************************* -->
<!ELEMENT introduction (content)>
<!ATTLIST introduction
	id CDATA #IMPLIED
	name CDATA #IMPLIED
>
<!ELEMENT remarks (content)>
<!ATTLIST remarks
	id CDATA #IMPLIED
	name CDATA #IMPLIED
>
<!ELEMENT commentary (content)>
<!ATTLIST commentary
	id CDATA #IMPLIED
