      <level5 id="wsb_12004026_001">
        <front>
          <biblioData type="T">
            <title>
              <mainTitle>영의정 남구만이 민암·장희재를 관대하게 처리한 것을 논한 기사</mainTitle>
            </title>
            <docNo level="n" name="titleno">001</docNo>
            <date>
              <dateOccured date="1694-04-26L0" type="서기"/>
            </date>
            <source>
              <mainTitle type="태백산사고본">肅宗實錄補闕正誤 28책 26권</mainTitle>
