    <level4 id="wsb_12012002">
      <front>
        <biblioData type="L">
          <title>
            <mainTitle>肅宗補闕正誤 20年 12月 2日</mainTitle>
          </title>
          <date>
            <dateOccured date="1694-12-02L0" type="서기"/>
            <dateOccured type="간지">乙未</dateOccured>
            <dateOccured type="재위연도">숙종 [보궐정오실록] 20년 12월 2일</dateOccured>
