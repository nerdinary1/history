      <level5 id="wsb_10810025_001">
        <front>
          <biblioData type="T">
            <title>
              <mainTitle>어영 대장 김익훈이 정원의 아방으로 나아가 거짓으로 상변하다</mainTitle>
            </title>
            <docNo level="n" name="titleno">001</docNo>
            <date>
              <dateOccured date="1682-10-25L0" type="서기"/>
            </date>
            <source>
              <mainTitle type="태백산사고본">肅宗實錄補闕正誤 13책 13권</mainTitle>
              <page begin="1장 A면"/>
            </source>
            <source>
              <mainTitle type="국편영인본">肅宗實錄補闕正誤 38책</mainTitle>
              <page begin="618면"/>
