      <level5 id="wsb_10804027_001">
        <front>
          <biblioData type="T">
            <title>
              <mainTitle>허목의 졸기</mainTitle>
            </title>
            <docNo level="n" name="titleno">001</docNo>
            <date>
              <dateOccured date="1682-04-27L0" type="서기"/>
            </date>
            <source>
              <mainTitle type="태백산사고본">肅宗實錄補闕正誤 12책 13권</mainTitle>
              <page begin="1장 B면"/>
            </source>
            <source>
              <mainTitle type="국편영인본">肅宗實錄補闕正誤 38책</mainTitle>
