      <level5 id="wsb_12311009_001">
        <front>
          <biblioData type="T">
            <title>
              <mainTitle>이세귀를 장령으로 삼다</mainTitle>
            </title>
            <docNo level="n" name="titleno">001</docNo>
            <date>
              <dateOccured date="1697-11-09L0" type="서기"/>
            </date>
            <source>
