      <front>
        <biblioData type="L">
          <title>
            <mainTitle>肅宗補闕正誤 15年 6月 3日</mainTitle>
          </title>
          <date>
            <dateOccured date="1689-06-03L0" type="서기"/>
            <dateOccured type="간지">戊辰</dateOccured>
            <dateOccured type="재위연도">숙종 [보궐정오실록] 15년 6월 3일</dateOccured>
            <dateOccured type="개국연호">조선 개국(開國) 298년 6월 3일</dateOccured>
