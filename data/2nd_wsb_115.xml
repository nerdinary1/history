      <level5 id="wsb_11503128_001">
        <front>
          <biblioData type="T">
            <title>
              <mainTitle>전 영의정 김수항의 졸기</mainTitle>
            </title>
            <docNo level="n" name="titleno">001</docNo>
            <date>
              <dateOccured date="1689-03-28L1" type="서기"/>
            </date>
            <source>
              <mainTitle type="태백산사고본">肅宗實錄補闕正誤 22책 20권</mainTitle>
