      <level5 id="wsb_10201002_001">
        <front>
          <biblioData type="T">
            <title>
              <mainTitle>승지 심광수에게 이조 참판을 추증하다</mainTitle>
            </title>
            <docNo level="n" name="titleno">001</docNo>
            <date>
              <dateOccured date="1676-01-02L0" type="서기"/>
            </date>
            <source>
